{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "moduleResolution": "node",
        "lib": ["es6", "dom"],
        "sourceMap": true,
        "noImplicitReturns": true,
        "rootDir": "src",
        "outDir": "dist",
        "jsx": "react"
    },
    "include": ["**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}